<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatisorg//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ahn.mapper.PointMapper">
	<update id="updatePoint">
		update tbl_user set upoint = upoint + #{point} where 
		uid=#{uid}
	</update>
</mapper>